CREATE TABLE `ws_GoogleCheckoutLog` (
 `RowGuid` varchar(36) NOT NULL, 
 `CreatedUtc` datetime NOT NULL,
 `SiteGuid` varchar(36) NOT NULL,
 `UserGuid` varchar(36) NOT NULL,
 `StoreGuid` varchar(36) NOT NULL,
 `CartGuid` varchar(36) NOT NULL,
 `NotificationType` VarChar(255) NULL,
 `RawResponse` text NULL,
 `SerialNumber` VarChar(50) NULL,
 `GTimestamp` datetime NULL,
 `OrderNumber` VarChar(50) NULL,
 `BuyerId` VarChar(50) NULL,
 `FullfillState` VarChar(50) NULL,
 `FinanceState` VarChar(50) NULL,
 `EmailListOptIn` tinyint(1) unsigned NOT NULL,
 `AvsResponse` VarChar(5) NULL,
 `CvnResponse` VarChar(5) NULL,
 `AuthExpDate` datetime NULL,
 `AuthAmt` Decimal(15,4) NOT NULL,
 `DiscountTotal` Decimal(15,4) NOT NULL,
 `ShippingTotal` Decimal(15,4) NOT NULL,
 `TaxTotal` Decimal(15,4) NOT NULL,
 `OrderTotal` Decimal(15,4) NOT NULL,
 `LatestChgAmt` Decimal(15,4) NOT NULL,
 `TotalChgAmt` Decimal(15,4) NOT NULL,
 `LatestRefundAmt` Decimal(15,4) NOT NULL,
 `TotalRefundAmt` Decimal(15,4) NOT NULL,
 `LatestChargeback` Decimal(15,4) NOT NULL,
 `TotalChargeback` Decimal(15,4) NOT NULL,
PRIMARY KEY (`RowGuid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `ws_PayPalLog` (
 `RowGuid` varchar(36) NOT NULL, 
 `CreatedUtc` datetime NOT NULL,
 `SiteGuid` varchar(36) NOT NULL,
 `UserGuid` varchar(36) NOT NULL,
 `StoreGuid` varchar(36) NOT NULL,
 `CartGuid` varchar(36) NOT NULL,
 `RequestType` VarChar(255) NOT NULL,
 `ApiVersion` VarChar(50) NULL,
 `RawResponse` text NULL,
 `Token` VarChar(50) NULL,
 `PayerId` VarChar(50) NULL,
 `TransactionId` VarChar(50) NULL,
 `PaymentType` VarChar(10) NULL,
 `PaymentStatus` VarChar(50) NULL,
 `PendingReason` VarChar(255) NULL,
 `ReasonCode` VarChar(50) NULL,
 `CurrencyCode` VarChar(50) NULL,
 `ExchangeRate` Decimal(15,4) NOT NULL,
 `CartTotal` Decimal(15,4) NOT NULL,
 `PayPalAmt` Decimal(15,4) NOT NULL,
 `TaxAmt` Decimal(15,4) NOT NULL,
 `FeeAmt` Decimal(15,4) NOT NULL,
 `SettleAmt` Decimal(15,4) NOT NULL,
PRIMARY KEY (`RowGuid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
