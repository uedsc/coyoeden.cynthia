CREATE TABLE `cy_ContentTemplate` ( `Guid` varchar(36) NOT NULL PRIMARY KEY,  `SiteGuid` varchar(36) NOT NULL, `Title` varchar(255) NOT NULL, `ImageFileName` varchar(255) NULL, `Description` text NULL, `Body` text NOT NULL, `AllowedRoles` text NULL, `CreatedByUser` varchar(36) NOT NULL, `LastModUser` varchar(36) NOT NULL, `CreatedUtc` datetime NOT NULL, `LastModUtc` datetime NOT NULL);CREATE INDEX idxCTTitle ON cy_ContentTemplate(`Title`);CREATE TABLE `cy_ContentStyle` ( `Guid` varchar(36) NOT NULL PRIMARY KEY,  `SiteGuid` varchar(36) NOT NULL, `Name` varchar(100) NOT NULL, `Element` varchar(50) NOT NULL, `CssClass` varchar(50) NOT NULL, `SkinName` varchar(100) NOT NULL, `IsActive` INTEGER NOT NULL, `CreatedUtc` datetime NOT NULL, `LastModUtc` datetime NOT NULL, `CreatedBy` varchar(36) NOT NULL, `LastModBy` varchar(36) NOT NULL);CREATE INDEX idxCStyleName ON cy_ContentStyle(`Name`);CREATE INDEX idxCStyleSkin ON cy_ContentStyle(`SkinName`);INSERT INTO cy_SiteSettingsExDef(KeyName,DefaultValue,GroupName,SortOrder) VALUES('WebSnaprKey','','APIKeys', 100);